{
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1,
  },
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "/bin/zsh",
      "args": [
        "-l",
        "-i"
      ]
    }
  },
  "terminal.integrated.fontFamily": "MesloLGS NF",
  // VSCODE Vim
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  // "vim.visualModeKeyBindings": [
  // {
  // "before": ["j", "k"],
  // "after": ["<Esc>"]
  // }
  // ],
  "vim.handleKeys": {
    // "<C-a>": false,
    "<C-f>": false,
    "<C-c>": false,
    // "<C-x>": false,
    "<C-b>": false,
    "<C-j>": true,
    "<C-k>": true,
  },
  "vim.highlightedyank.enable": true,
  "vim.gdefault": true,
  "vim.highlightedyank.color": "#a9dc7660",
  "vim.highlightedyank.duration": 250,
  "vim.sneak": true,
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.useCtrlKeys": true,
  "vim.vimrc.enable": false,
  "vim.leader": "<space>",
  // "vim.vimrc.path": "/Users/sivan/.vimrc",
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "ctrl+v"
      ],
      "after": [
        "ctrl+r",
        "\""
      ]
    },
    {
      "before": [
        "alt+j"
      ],
      "after": [
        "ctrl+n"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    [
        {
            "before": ["U"],
            "commands":["extension.vim_ctrl+r"]
        }
    ],
    {
      "before": [
        "g",
        "q"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    {
      "before": [
        "g",
        "f"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "6",
        "j"
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "6",
        "k"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "Y"
      ],
      "after": [
        "y",
        "$"
      ]
    },
    { // emulate sneak with easymotion (no label mode in sneak yet)
    "before": [ "<leader>", "s" ],
    "after": [ "<leader>", "<leader>", "2", "f" ]
    },
    {
      "before": [ "<leader>", "S" ],
      "after": [ "<leader>", "<leader>", "2", "F" ]
    },
    {
      "before": [
        "<Leader>",
        "c"
      ],
      "commands": [
        "workbench.action.debug.configure"
      ]
    },
    {
      "before": [
        "<Leader>",
        "z"
      ],
      "commands": [
        "workbench.action.toggleZenMode"
      ]
    },
    {
      "before": [
        "<Leader>",
        "g"
      ],
      "commands": [
        "magit.status"
      ]
    },
    {
      "before": [
        "leader",
        ","
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "<Leader>",
        "l"
      ],
      "commands": [
        "workbench.action.navigateForward"
      ]
    },
    {
      "before": [
        "<Leader>",
        "h"
      ],
      "commands": [
        "workbench.action.navigateBack"
      ]
    },
    {
      "before": [
        "<Leader>",
        "."
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<Leader>",
        "d"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "leader",
        "D"
      ],
      "commands": [
        "workbench.action.closeOtherEditors"
      ]
    },
    {
      "before": [
        "leader",
        "/"
      ],
      "commands": [
        "workbench.action.splitEditor"
      ]
    },
    {
      "before": [
        "leader",
        "w"
      ],
      "after": [
        "leader",
        "leader",
        "w"
      ]
    },
    {
      "before": [
        "leader",
        "b"
      ],
      "after": [
        "leader",
        "leader",
        "b"
      ]
    },
    {
      "before": [
        "leader",
        "k"
      ],
      "after": [
        "leader",
        "leader",
        "k"
      ]
    },
    {
      "before": [
        "leader",
        "j"
      ],
      "after": [
        "leader",
        "leader",
        "j"
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "e"
      ]
    },
    {
      "before": [
        "<leader>",
        "o"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "f"
      ]
    },
    {
      "before": [
        "leader",
        "n"
      ],
      "commands": [
        "workbench.action.moveEditorToNewWindow"
      ]
    },
    {
      "before": [
        "leader",
        "p"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "p",
      ],
      "after": [
        "p",
        "g",
        "v",
        "y"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "6",
        "j"
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "6",
        "k"
      ]
    },
    {
      "before": [
        "<Leader>",
        ","
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
  ],



  // Theming
  "workbench.colorTheme": "Catppuccin Mocha",
  "catppuccin.workbenchMode": "flat",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.colorCustomizations": {
    "[Default Dark Modern]": {
      "editorCursor.foreground": "#a6c5e0", // Cursor color
      "editorCursor.background": "#020202", // Cursor color
    },
    "[Atom Background Modified]": {
      "editorCursor.background": "#ffffff", // Cursor color
    },
    "[Catppuccin *]": {
      "editorCursor.foreground": "#AC8DD8", // Cursor color
      "editorCursor.background": "#ffffff", // Cursor color
    },
    "[Tokyo *]": {
      // "editorCursor.foreground": "#8ccaeee3", // Cursor color
      "editorCursor.foreground": "#8c7fca", // Cursor color
      "editorCursor.background": "#ffffff", // Cursor color
      "editor.findMatchHighlightBackground": "#9774d051", // selected color in "find"
      "editor.selectionHighlightBorder": "#6b7295",
      // "editor.selectionBackground": "#4b0808",
      "editor.selectionBackground": "#8d45cd46",
      "editor.wordHighlightStrongBorder": "#7982a9",
      "sideBar.foreground": "#dcdee5", // sidebar font color
      "panelTitle.activeBorder": "#3d59a1",
      "panelTitle.activeForeground": "#bdc7f0",
      "panelTitle.inactiveForeground": "#959cbd",
      "tab.activeForeground": "#bdc7f0",
      "tab.activeBackground": "#4047697e",
      "tab.inactiveForeground": "#959cbd",
      "breadcrumb.foreground": "#959cbd",
      "breadcrumb.focusForeground": "#bdc7f0",
      "breadcrumb.activeSelectionForeground": "#f0ebbd",
      "statusBar.foreground": "#bdc7f0",
      "list.focusForeground": "#bdc7f0",
      "list.hoverForeground": "#bdc7f0",
      "list.activeSelectionForeground": "#bdc7f0",
      "list.inactiveSelectionForeground": "#bdc7f0",
      "list.inactiveSelectionBackground": "#202330",
      "dropdown.foreground": "#959cbd",
      "menu.foreground": "#bdc7f0",
      "menubar.selectionForeground": "#bdc7f0",
      "input.foreground": "#959cbd",
      "input.placeholderForeground": "#959cbd",
      "activityBar.foreground": "#bdc7f0",
      "activityBar.inactiveForeground": "#787c99",
      "gitDecoration.ignoredResourceForeground": "#9fa3bc",
    },
  },
  "editor.tokenColorCustomizations": {
    "[Atom Background Modified]": {
      "comments": "#6cd5718a",
    },
    "[Tokyo *]": {
      // "comments": "#6cd5718a",
      "comments": "#8eba908a",
    },
    "[Catpp*]":{
      "comments": "#8eba908a",
    }
  },
  "editor.cursorBlinking": "smooth",


  // Others
  "git.ignoreMissingGitWarning": true,
  "editor.accessibilitySupport": "off",
  "editor.guides.bracketPairs": true,
  "editor.guides.highlightActiveIndentation": true,
  "editor.suggest.preview": true,
  "editor.guides.indentation": true,
  "gitlens.hovers.currentLine.over": "line",
  // "audioCues.terminalQuickFix": "on",
  "git.autofetch": true,
  "editor.semanticHighlighting.enabled": true,
  // "editor.fontSize": 12,
  // "debug.console.fontSize": 13.5,
  // "terminal.integrated.fontSize": 13.5,
  "window.zoomLevel": -0.3,
  "debug.openDebug": "neverOpen",
  "explorer.confirmDelete": false,
  "debug.inlineValues": "on",
  // "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.cursorSurroundingLines": 5,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "workbench.activityBar.location": "bottom",
  "python.createEnvironment.trigger": "off",
  "files.exclude": {// Hide pycach
    "**/__pycache__": true
  },
  "debug.console.acceptSuggestionOnEnter": "off",
  "jupyter.notebookFileRoot": "${workspaceFolder}",
  "jupyter.debugJustMyCode": false,
  "notebook.lineNumbers": "on",
  "git.openRepositoryInParentFolders": "never",
  "diffEditor.ignoreTrimWhitespace": false,
  "cSpell.enabled": true,
  "calva.prettyPrintingOptions": {
    "printEngine": "pprint",
    "enabled": true,
    "width": 120,
    "maxLength": 50
  },
  "calva.paredit.defaultKeyMap": "original",
  "[clojure]": {
    "editor.defaultFormatter": "betterthantomorrow.calva"
  },
  "cSpell.userWords": [
    "litellm",
    "OMNI",
    "openai"
  ],
  "git.confirmSync": false,
  "terminal.integrated.inheritEnv": true,
}
